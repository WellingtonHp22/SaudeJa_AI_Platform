# Configurações de Segurança e LGPD
SECRET_KEY=seu-secret-key-super-seguro-aqui-change-in-production
ANONYMIZE_DATA=true
DATA_RETENTION_DAYS=365
ENABLE_USER_CONSENT=true
LOG_LEVEL=INFO

# Configurações de IA Avançada
ENABLE_AI_CLUSTERING=true
ENABLE_SENTIMENT_ANALYSIS=true
ENABLE_PREDICTIVE_ANALYTICS=true

# Configurações de API e Web Crawling
MAX_REQUESTS_PER_HOUR=100
API_RATE_LIMIT=60
CRAWL_DELAY=1.0

# Configurações de Cache
CACHE_DURATION_HOURS=24

# Modo de Desenvolvimento/Produção
DEBUG_MODE=false
PRODUCTION_MODE=false
